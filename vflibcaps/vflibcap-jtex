  (font-class ascii-jtex-kanji) (kanji-font-magnification 0.95)
  (metric-adjustment-file "none.adj"))
(define-macro illust-tcommon
  (font-class ascii-jtex-kanji) (kanji-font-magnification 0.95)
  (metric-adjustment-file "tnone.adj"))

